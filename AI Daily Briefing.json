    {
      "parameters": {
        "jsCode": "const body = $(\"Webhook: Update Config\").first().json.body || $(\"Webhook: Update Config\").first().json;\nlet existing = { topics: [], tickers: [] };\n\ntry {\n  const binaryData = $input.first().binary?.data;\n  if (binaryData) {\n    const buffer = Buffer.from(binaryData.data, 'base64');\n    existing = JSON.parse(buffer.toString());\n  }\n} catch (e) {\n  // No existing config\n}\n\n// Reset functionality\nif (body.clear === true || body.reset === true) {\n  existing = { topics: [], tickers: [] };\n} else {\n  // Toggle logic for topics\n  if (Array.isArray(body.topics)) {\n    body.topics.forEach(t => {\n      const index = existing.topics.indexOf(t);\n      if (index > -1) existing.topics.splice(index, 1);\n      else existing.topics.push(t);\n    });\n  }\n  \n  // Toggle logic for tickers\n  if (Array.isArray(body.tickers)) {\n    body.tickers.forEach(t => {\n      const index = existing.tickers.indexOf(t);\n      if (index > -1) existing.tickers.splice(index, 1);\n      else existing.tickers.push(t);\n    });\n  }\n}\n\nreturn {\n  binary: {\n    data: {\n      data: Buffer.from(JSON.stringify(existing)).toString('base64'),\n      mimeType: 'application/json',\n      fileName: 'briefing_config.json'\n    }\n  },\n  // Return the new state so the user can see what's currently active\n  new_config: existing\n};"
      },
