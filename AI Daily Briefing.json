    {
      "parameters": {
        "jsCode": "\n// Handle both API response format (with .data) and direct file format\nconst inputJson = $input.first().json;\nconst inputData = inputJson.data || (inputJson.body && inputJson.body.config);\nlet rawTopics = inputData.topics || [];\nconst tickers = inputData.tickers || [];\nconst userId = inputData.userId || (inputJson.body && inputJson.body.userId) || null;\n\n// Extract names from objects if necessary and filter for enabled topics\nconst topics = rawTopics.map(t => {\n  if (typeof t === 'string') return t;\n  return t.enabled !== false ? t.name : null;\n}).filter(t => t !== null);\n\nreturn {\n  topics,\n  tickers,\n  userId,\n  rssQuery: topics.length > 0 ? topics.join(' OR ') : \"market news\"\n};"
      },
